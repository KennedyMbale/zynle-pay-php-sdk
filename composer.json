{
    "name": "kennedymbale/zynle-pay-php-sdk",
    "description": "Professional PHP SDK for ZynlePay payment gateway API supporting Mobile Money and Card payments in Zambia.",
    "type": "library",
    "license": "MIT",
    "version": "2.0.0",
    "keywords": ["zynlepay", "payment", "gateway", "mobile money", "card", "php", "sdk", "Zambia"],
    "authors": [
        {
            "name": "Kennedy Mbale",
            "email": "kennedymbale4@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.0",
        "guzzlehttp/guzzle": "^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "phpstan/phpstan": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "ZynlePay\\": "src/ZynlePay/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ZynlePay\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
